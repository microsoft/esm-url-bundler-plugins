{
  "name": "@vscode/esm-url-plugin-common",
  "version": "1.0.0",
  "private": true,
  "description": "Shared utilities for esm-url bundler plugins",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "rollup": "^4.59.0"
  },
  "license": "MIT"
}
